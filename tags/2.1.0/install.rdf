    <!-- Front End MetaData -->
    <name>Music Player Minion 2</name>
    <description>Music Player Daemon (MPD) client.</description>
    <creator>Chris Seickel</creator>
    <homepageURL>http://code.google.com/p/musicpm/</homepageURL>

    <contributor>Aurélien Requiem</contributor>
    <contributor>Alexandre Moore (icons)</contributor>
    <contributor>Tango Desktop Project (icons)</contributor>

    <translator>Aurélien Requiem (fr-FR)</translator>
    <translator>Roberto Ripio (es-ES)</translator>
    <translator>Rüdiger Hausmann (de-DE)</translator>

  </rdf:Description>
</rdf:RDF>
